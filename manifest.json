{
  "manifest_version": 2,
  "name": "二维码(QR Code)生成器",
  "version": "0.1.0",
  "description": "快速生成当前页面二维码，扫码二维码即可访问当前页面",
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png",
    "128": "image/icon128.png"
  },
  "browser_action": {
    "default_title": "二维码(sQR Code)生成器",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
